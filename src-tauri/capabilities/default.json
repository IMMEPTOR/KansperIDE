{
  "$schema": "../gen/schemas/desktop-capability.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:window:default",
    "core:path:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-confirm",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-read-dir",
    "fs:allow-rename",
    "fs:default",
    "shell:default"
  ]
}